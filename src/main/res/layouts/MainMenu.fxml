<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>


<MenuBar fx:id="MainMenu" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controllers.MenuBarController">
  <menus>
    <Menu mnemonicParsing="false" text="File">
      <items>
        <MenuItem fx:id="quitItem" mnemonicParsing="false" onAction="#quitApplication" text="quit" />
      </items>
    </Menu>
    <Menu mnemonicParsing="false" text="Edit">
      <items>
        <MenuItem mnemonicParsing="false" text="cut" />
            <MenuItem mnemonicParsing="false" text="copy" />
            <MenuItem mnemonicParsing="false" text="paste" />
      </items>
    </Menu>
  </menus>
</MenuBar>
