<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>

<MenuBar fx:id="MainMenu" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.github.michael_bailey.client.MenuBar.MenuBarController">
  <menus>
    <Menu mnemonicParsing="false" text="File">
      <items>
        <MenuItem fx:id="quitItem" mnemonicParsing="false" onAction="#quitApplication" text="quit" />
      </items>
    </Menu>
    <Menu mnemonicParsing="false" text="Edit">
      <items>
        <MenuItem mnemonicParsing="false" text="cut" />
            <MenuItem mnemonicParsing="false" text="copy" />
            <MenuItem mnemonicParsing="false" text="paste" />
      </items>
    </Menu>
  </menus>
</MenuBar>
